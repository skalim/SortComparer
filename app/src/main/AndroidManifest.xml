<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="saad.sortcomparer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".firstscreen.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".secondscreen.SecondScreen"
            android:label="@string/title_activity_second_screen" />
        <activity
            android:name=".thirdscreen.ThirdScreen"
            android:label="@string/title_activity_third_screen" />
        <activity
            android:name=".resultsscreen.ResultsActivity"
            android:label="@string/title_activity_results"></activity>
    </application>

</manifest>
